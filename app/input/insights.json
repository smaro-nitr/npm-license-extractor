{
    "name": "Insights",
    "version": "5.0.0",
    "description": "Alphonso Insights Platform",
    "repository": {
      "type": "git",
      "url": "git://github.com/AlphonsoCode/insights.git"
    },
    "engines": {
      "npm": ">=3",
      "node": ">=5"
    },
    "author": "Alphonso",
    "license": "UNLICENSED",
    "scripts": {
      "analyze:clean": "rimraf stats.json",
      "preanalyze": "npm run analyze:clean",
      "analyze": "node ./internals/scripts/analyze.js",
      "extract-intl": "babel-node --presets latest,stage-0 -- ./internals/scripts/extract-intl.js",
      "npmcheckversion": "node ./internals/scripts/npmcheckversion.js",
      "preinstall": "npm run npmcheckversion",
      "postinstall": "npm run build:dll",
      "prebuild": "npm run build:clean",
      "build": "cross-env NODE_ENV=production webpack --config internals/webpack/webpack.prod.babel.js --color -p",
      "build:test": "cross-env NODE_ENV=test webpack --config internals/webpack/webpack.test.babel.js --color -p",
      "build:clean": "npm run test:clean && rimraf ./build",
      "build:dll": "node ./internals/scripts/dependencies.js",
      "start": "cross-env NODE_ENV=development node server",
      "start:docker": "docker run --rm -d -p 8000:8000 -v \"$(pwd)\":/app -w /app node:8 sh -c 'rm -Rf node_modules/; npm install; npm run build:dll; npm run start'",
      "start:custom": "cross-env NODE_ENV=custom node server",
      "start:debug": "cross-env NODE_ENV=development node debug server",
      "start:tunnel": "cross-env NODE_ENV=development ENABLE_TUNNEL=true node server",
      "start:debug-prod": "cross-env NODE_ENV=productionNoEmail node debug server",
      "start:production": "npm run test && npm run build && npm run start:prod",
      "start:prod": "cross-env NODE_ENV=production node server",
      "presetup": "npm i chalk shelljs",
      "setup": "node ./internals/scripts/setup.js",
      "postsetup": "npm run build:dll",
      "clean": "shjs ./internals/scripts/clean.js",
      "clean:all": "npm run analyze:clean && npm run test:clean && npm run build:clean",
      "generate": "plop --plopfile internals/generators/index.js",
      "lint": "npm run lint:js",
      "lint:eslint": "eslint --ignore-path .gitignore --ignore-pattern internals/scripts",
      "lint:js": "npm run lint:eslint -- . ",
      "lint:staged": "lint-staged",
      "pretest": "npm run test:clean && npm run lint",
      "test:clean": "rimraf ./coverage",
      "test": "cross-env NODE_ENV=test jest --verbose --noStackTrace --colors",
      "test:precommit": "cross-env NODE_ENV=test jest --verbose --cache --noStackTrace --findRelatedTests --colors",
      "test:watch": "cross-env NODE_ENV=test jest --watchAll",
      "coveralls": "cat ./coverage/lcov.info | coveralls",
      "docs": "rdd app/components/ --readmeFile docs/README_COMPONENTS.md --template docs/readme_components_template.md",
      "start:test": "cross-env NODE_ENV=test node server"
    },
    "jest": {
      "setupFiles": [
        "./test-setup.js"
      ],
      "collectCoverageFrom": [
        "app/**/*.{js,jsx}",
        "!app/**/*.test.{js,jsx}",
        "!app/*/RbGenerated*/*.{js,jsx}",
        "!app/app.js",
        "!app/routes.js"
      ],
      "coverageThreshold": {
        "global": {
          "statements": 0,
          "branches": 0,
          "functions": 0,
          "lines": 0
        }
      },
      "moduleDirectories": [
        "node_modules",
        "app"
      ],
      "moduleNameMapper": {
        ".*\\.(css|less|styl|scss|sass)$": "<rootDir>/internals/mocks/cssModule.js",
        ".*\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/internals/mocks/image.js"
      },
      "setupTestFrameworkScriptFile": "<rootDir>/internals/testing/test-bundler.js",
      "testRegex": "tests/.*\\.test\\.js$",
      "testResultsProcessor": "./node_modules/jest-junit-reporter"
    },
    "lint-staged": {
      "*.js": [
        "lint:eslint",
        "test:precommit"
      ]
    },
    "pre-commit": [
      "lint:staged"
    ],
    "babel": {
      "presets": [
        [
          "latest",
          {
            "es2015": {
              "modules": false
            }
          }
        ],
        "react",
        "stage-0"
      ],
      "env": {
        "production": {
          "only": [
            "app"
          ],
          "plugins": [
            "transform-react-remove-prop-types",
            "transform-react-constant-elements",
            "transform-react-inline-elements",
            "transform-async-to-generator"
          ]
        },
        "development": {
          "plugins": [
            "transform-async-to-generator"
          ]
        },
        "test": {
          "plugins": [
            "transform-es2015-modules-commonjs",
            "transform-async-to-generator",
            "dynamic-import-node"
          ]
        }
      }
    },
    "eslintConfig": {
      "parser": "babel-eslint",
      "extends": "airbnb",
      "env": {
        "browser": true,
        "node": true,
        "jest": true,
        "es6": true
      },
      "plugins": [
        "redux-saga",
        "react",
        "jsx-a11y"
      ],
      "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
          "jsx": true
        }
      },
      "rules": {
        "arrow-parens": 0,
        "jsx-a11y/alt-text": 0,
        "jsx-a11y/anchor-is-valid": 0,
        "react/jsx-curly-brace-presence": 0,
        "jsx-a11y/no-noninteractive-element-interactions": 0,
        "jsx-a11y/mouse-events-have-key-events": 0,
        "jsx-a11y/media-has-caption": 0,
        "jsx-a11y/click-events-have-key-events": 0,
        "no-unused-expressions": 0,
        "no-case-declarations": 0,
        "no-plusplus": 0,
        "global-require": 0,
        "object-curly-newline": 0,
        "arrow-body-style": 0,
        "comma-dangle": 0,
        "import/extensions": 0,
        "import/imports-first": 0,
        "import/newline-after-import": 0,
        "import/no-dynamic-require": 0,
        "import/no-extraneous-dependencies": 0,
        "no-script-url": 0,
        "react/no-array-index-key": 0,
        "import/no-named-as-default": 0,
        "import/no-unresolved": 2,
        "react/jsx-wrap-multilines": 0,
        "import/prefer-default-export": 0,
        "react/prefer-stateless-function": 0,
        "indent": 0,
        "brace-style": 0,
        "function-paren-newline": 0,
        "react/jsx-no-bind": 0,
        "react/no-find-dom-node": 0,
        "jsx-a11y/aria-props": 2,
        "jsx-a11y/heading-has-content": 0,
        "jsx-a11y/label-has-for": 2,
        "no-else-return": 0,
        "jsx-a11y/role-has-required-aria-props": 2,
        "react/require-default-props": 0,
        "jsx-a11y/role-supports-aria-props": 2,
        "max-len": 0,
        "space-infix-ops": 0,
        "no-nested-ternary": 0,
        "no-bitwise": 0,
        "newline-per-chained-call": 0,
        "no-param-reassign": 0,
        "no-mixed-operators": 0,
        "no-undef": 0,
        "no-confusing-arrow": 0,
        "no-console": 1,
        "no-unused-vars": 0,
        "no-use-before-define": 0,
        "no-useless-constructor": 0,
        "jsx-a11y/no-static-element-interactions": 0,
        "prefer-template": 0,
        "prefer-destructuring": 0,
        "class-methods-use-this": 0,
        "react/forbid-prop-types": 0,
        "react/jsx-first-prop-new-line": [
          2,
          "multiline"
        ],
        "react/jsx-filename-extension": 0,
        "react/jsx-no-target-blank": 0,
        "react/jsx-curly-spacing": 0,
        "react/require-extension": 0,
        "react/self-closing-comp": 0,
        "redux-saga/no-yield-in-race": 2,
        "redux-saga/yield-effects": 2,
        "require-yield": 0,
        "import/no-webpack-loader-syntax": 0,
        "react/no-string-refs": 0
      },
      "settings": {
        "import/resolver": {
          "webpack": {
            "config": "./internals/webpack/webpack.prod.babel.js"
          }
        }
      }
    },
    "dllPlugin": {
      "path": "node_modules/react-boilerplate-dlls",
      "exclude": [
        "chalk",
        "compression",
        "cross-env",
        "express",
        "ip",
        "minimist",
        "sanitize.css"
      ],
      "include": [
        "core-js",
        "lodash",
        "eventsource-polyfill"
      ]
    },
    "dependencies": {
      "@material-ui/core": "3.2.2",
      "@material-ui/icons": "3.0.1",
      "@redsift/d3-rs-venn": "0.0.3",
      "alphonso": "github:AlphonsoCode/WebModules#V1.8.6",
      "babel-polyfill": "6.20.0",
      "body-parser": "1.17.2",
      "chalk": "1.1.3",
      "chroma-js": "1.3.4",
      "classnames": "2.2.5",
      "compression": "1.6.2",
      "cookie-parser": "1.4.3",
      "cross-env": "3.1.3",
      "d3": "4.13.0",
      "dropbox": "^4.0.9",
      "express": "4.14.0",
      "file-saver": "^2.0.1",
      "font-awesome": "4.7.0",
      "google-map-react": "0.24.0",
      "halogen": "^0.2.0",
      "highcharts": "6.1.2",
      "immutable": "3.8.1",
      "intl": "1.2.5",
      "invariant": "2.2.2",
      "ip": "1.1.4",
      "jest-junit-reporter": "^1.1.0",
      "jest-styled-components": "^6.2.1",
      "jquery": "3.2.1",
      "jsonwebtoken": "8.2.1",
      "jszip": "^3.2.1",
      "jszip-utils": "0.0.2",
      "jwt-decode": "2.2.0",
      "lodash": "^4.17.5",
      "minimist": "1.2.0",
      "mobile-detect": "1.4.3",
      "moment": "^2.19.3",
      "node-sass": "4.5.3",
      "password-validator": "4.1.1",
      "query-string": "5.0.0",
      "randomstring": "1.1.5",
      "react": "15.4.1",
      "react-bootstrap": "0.31.2",
      "react-burger-menu": "2.1.11",
      "react-copy-to-clipboard": "5.0.1",
      "react-date-range": "0.9.4",
      "react-dom": "15.4.1",
      "react-dropzone": "6.0.3",
      "react-helmet": "5.2.0",
      "react-highcharts": "16.0.2",
      "react-html5video": "^2.5.1",
      "react-intl": "2.1.5",
      "react-measure": "^2.1.2",
      "react-redux": "4.4.6",
      "react-router": "3.0.0",
      "react-router-redux": "4.0.6",
      "react-router-scroll": "0.4.1",
      "react-select": "1.2.1",
      "react-slick": "0.14.11",
      "react-spinkit": "3.0.0",
      "react-stickynode": "1.3.1",
      "react-table": "^6.8.6",
      "react-test-renderer": "15.6.1",
      "redux": "3.6.0",
      "redux-logger": "3.0.6",
      "redux-saga": "0.15.0",
      "request": "2.83.0",
      "reselect": "2.5.4",
      "sanitize.css": "4.1.0",
      "sass-loader": "6.0.6",
      "shelljs": "0.7.5",
      "slick-carousel": "^1.8.1",
      "slugify": "1.2.1",
      "styled-components": "^3.0.2",
      "url-search-params": "0.10.0",
      "validator": "10.2.0",
      "warning": "3.0.0",
      "whatwg-fetch": "2.0.1"
    },
    "devDependencies": {
      "babel-cli": "6.18.0",
      "babel-core": "6.21.0",
      "babel-eslint": "7.1.1",
      "babel-loader": "7.1.1",
      "babel-plugin-dynamic-import-node": "1.0.0",
      "babel-plugin-react-intl": "2.2.0",
      "babel-plugin-react-transform": "2.0.2",
      "babel-plugin-transform-async-to-generator": "6.24.1",
      "babel-plugin-transform-es2015-modules-commonjs": "6.18.0",
      "babel-plugin-transform-react-constant-elements": "6.9.1",
      "babel-plugin-transform-react-inline-elements": "6.8.0",
      "babel-plugin-transform-react-remove-prop-types": "0.2.11",
      "babel-preset-env": "1.6.0",
      "babel-preset-latest": "6.16.0",
      "babel-preset-react": "6.16.0",
      "babel-preset-react-hmre": "1.1.1",
      "babel-preset-stage-0": "6.16.0",
      "cheerio": "0.22.0",
      "circular-dependency-plugin": "2.0.0",
      "coveralls": "2.11.15",
      "css-loader": "0.26.1",
      "enzyme": "2.6.0",
      "eslint": "^4.11.0",
      "eslint-config-airbnb": "16.1.0",
      "eslint-config-airbnb-base": "12.1.0",
      "eslint-import-resolver-webpack": "0.8.3",
      "eslint-plugin-import": "2.7.0",
      "eslint-plugin-jsx-a11y": "6.0.2",
      "eslint-plugin-react": "7.4.0",
      "eslint-plugin-redux-saga": "^0.9.0",
      "eventsource-polyfill": "0.9.6",
      "exports-loader": "0.6.3",
